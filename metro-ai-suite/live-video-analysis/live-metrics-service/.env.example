# Metrics Service Environment Configuration
# Copy this file to .env and customize as needed

# Project name for Docker Compose
PROJECT_NAME=metrics-stack

# Service port
METRICS_PORT=9090

# Prometheus metrics port (from collector)
PROMETHEUS_PORT=9273

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# CORS allowed origins (comma-separated)
CORS_ORIGINS=*

# Docker registry (leave empty for local builds)
REGISTRY=

# Image tag
TAG=latest
